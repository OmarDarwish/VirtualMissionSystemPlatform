<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapping PUBLIC "-//EXOLAB/Castor Mapping DTD Version 1.0//EN"
                         "http://castor.org/mapping.dtd">
<mapping>
	<class name="edu.utdesign.rwc.vmsp.esb.Channel">
		<map-to xml="Channel" />
		<field name="id" type="integer">
			<bind-xml name="channelNum" node="element" />
		</field>
		<field name="isOn" get-method="isOn" type="boolean">
			<bind-xml name="isOn" node="element" />
		</field>
		<field name="channelType" type="string">
			<bind-xml name="channelType" node="element" />
		</field>
		<field name="phoneConnectionStatus" type="string">
			<bind-xml name="phoneStatus" node="element" />
		</field>
		<field name="radioConnectionStatus" type="string">
			<bind-xml name="radioStatus" node="element" />
		</field>
	</class>
</mapping>